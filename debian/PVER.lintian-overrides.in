# yes, we have to
@PVER@ binary: depends-on-python-minimal

@PVER@ binary: desktop-command-not-in-package
@PVER@ binary: menu-command-not-in-package

# license file referred by the standard library
@PVER@ binary: extra-license-file

@PVER@ binary: binary-without-manpage usr/bin/2to3-@VER@

# venv script interpreters are fine
@PVER@ binary: unusual-interpreter ./usr/lib/@PVER@/venv/scripts/*
@PVER@ binary: interpreter-not-absolute ./usr/lib/@PVER@/venv/scripts/*
